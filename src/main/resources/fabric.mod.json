{
  "schemaVersion": 1,
  "id": "archimc",
  "version": "${version}",
  "name": "ArchiMC",
  "description": "A Minecraft mod that adds a range of blocks and items, aimed to improve your experience building realistic cities. Idea inspired by the Cubed resource pack.",
  "authors": [
    "4FA"
  ],
  "contact": {
    "website": "jandt.nl",
    "repo": "https://github.com/jan-dt/ArchiMC"
  },
  "license": "MPL-2.0",
  "icon": "assets/archimc/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "nl.jandt.archimc.client.ArchiMCClient"
    ],
    "main": [
      "nl.jandt.archimc.ArchiMC"
    ]
  },
  "mixins": [
    "archimc.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
